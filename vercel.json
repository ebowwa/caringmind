{
  "version": 2,
  "builds": [
    {
      "src": "clients/caringmind-web-next15/package.json",
      "use": "@vercel/next",
      "config": {
        "projectRoot": "clients/caringmind-web-next15"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "clients/caringmind-web-next15/$1"
    }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./clients/caringmind-web-next15/"
}