{
    "version": 2,
    "builds": [
      {
        "src": "/clients/caringmindWeb/next.config.mjs",
        "use": "@vercel/next"
      },
      {
        "src": "backend/**/*",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/clients/caringmindWeb/$1"
      }
    ]
  }
  